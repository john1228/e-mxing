<input id="input-dim-1" type="file" multiple class="file-loading" accept="image/*">
<script>
    $("#input-dim-1").fileinput({
        uploadUrl: "/admin/services/<%=@service.id%>/service_photos",
        allowedFileExtensions: ["jpg", "jpeg", "png", "gif"],
        initialPreview: <%=raw @photos.map{|photo|
          "#{image_tag(photo.photo.url,width: '240px')}"
        }%>,
        initialPreviewConfig: [<%=raw @photos.map{|photo|%>
            {
                caption: '<%=photo.photo.file.filename%>',
                height: '120px',
                url: '<%=delete_service_photo_path(@service,photo)%>',
                key: <%=photo.id%>
            },
            <%}%>],
        overwriteInitial: false,
        minImageWidth: 50,
        minImageHeight: 50,
        maxFileCount: 8
    });
</script>