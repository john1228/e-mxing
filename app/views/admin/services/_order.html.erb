<div class="panel">
  <h3>私教订单情况</h3>

  <div class="panel_contents" id="order" style="min-height: 400px">﻿</div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        var series = {
            data: <%= raw orders%>,
            lines: {show: false},
            bars: {show: true, barWidth: 0.5, align: 'center'}
        }
        $.plot("#order", [series], {
            xaxis: {
                ticks: <%= raw coaches%>
            }
        });
    })
</script>

