<%= form_for([:admin, @physical_card], html: {class: "form-horizontal"}) do |f| %>
    <fieldset class="inputs">
      <legend><span>新建实体卡</span></legend>
      <div class="form-group form-group-sm">
        <label class="col-sm-2 control-label" for="service_interests">印刷卡号</label>

        <div class="col-sm-10">
          <%= f.text_field(:virtual_number, class: 'form-control', placeholder: '请输入实体卡印刷的卡号') %>
        </div>
      </div>
      <div class="form-group form-group-sm">
        <label class="col-sm-2 control-label" for="service_interests">内置卡号</label>

        <div class="col-sm-10">
          <%= f.text_field(:entity_number, class: 'form-control', placeholder: '请输入实体卡的内置卡号') %>
        </div>
      </div>

    </fieldset>
    <%= f.submit('提交') %>
<% end %>
