<!doctype html>
<html>
<head>
  <meta charset="UTF-8">
  <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport"/>
  <title>美型网</title>
  <%= stylesheet_link_tag 'share/style' %>
  <%= javascript_include_tag 'share/jquery-ui-1.10.3.min', 'share/jquery.fullPage.min', 'share/jquery.js', 'share/load', 'share/player' %>
  <script>
      $(document).ready(function () {
          $.fn.fullpage({});
