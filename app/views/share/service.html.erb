<script type="text/javascript">
    global_swipe_lock = 0;
    var config = {
        enable_music: 1,
        swipeCur: 0
    }
</script>

<div id="swipe">
  <ul>
    <li>
      <div class="li_bg"></div>
    </li>
  </ul>
</div>
<div id="musicWrap" class="music">
  <span class="text move hide"> </span><i id="audioBtn" data-src="music/music.mp3" class="btn_music on"></i></div>
